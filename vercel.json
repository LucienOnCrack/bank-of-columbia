{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "VERCEL_AUTOMATION_BYPASS": "1"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}